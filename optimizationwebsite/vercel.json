{
  "routes": [
    {
      "src": "/assets/images/(.*)",
      "headers": { "cache-control": "max-age=604800, public" },
      "dest": "/images/$1"
    },
    {
      "src": "/assets/bootstrap/css/(.*)",
      "headers": { "cache-control": "s-maxage=604800" },
      "dest": "/bootstrap/$1"
    }
  ]
}