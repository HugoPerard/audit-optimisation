{
  "headers": [
    {
      "source": "/assets/bootstrap/css/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=3600"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/assets/images/(.*)",
      "headers": { "cache-control": "s-maxage=604800" },
      "dest": "/images/$1"
    }
  ]
}